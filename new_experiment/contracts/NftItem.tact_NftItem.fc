(slice, int, int, slice, slice) $NftItem$_contract_init(int $index, slice $owner, int $pipeline_hash, slice $pipeline_raw, slice $collection) impure inline {
    var (($self'owner, $self'index, $self'pipeline_hash, $self'pipeline_raw, $self'collection)) = (null(), null(), null(), null(), null());
    $self'index = $index;
    $self'owner = $owner;
    $self'pipeline_hash = $pipeline_hash;
    $self'pipeline_raw = $pipeline_raw;
    $self'collection = $collection;
    return ($self'owner, $self'index, $self'pipeline_hash, $self'pipeline_raw, $self'collection);
}

