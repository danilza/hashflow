}

(slice, int, int, slice, slice) $NftItem$_contract_load() impure inline {
    slice $sc = get_data().begin_parse();
    int $loaded = $sc~load_int(1);
    if ($loaded) {
        return $sc~$NftItem$_load();
    }
    else {
        (int $index, slice $owner, int $pipeline_hash, slice $pipeline_raw, slice $collection) = $sc~$NftItem$init$_load();
        $sc.end_parse();
        return $NftItem$_contract_init($index, $owner, $pipeline_hash, $pipeline_raw, $collection);
    }
}

() $NftItem$_contract_store((slice, int, int, slice, slice) v) impure inline {
    builder b = begin_cell();
    b = b.store_int(true, 1);
    b = $NftItem$_store(b, v);
    set_data(b.end_cell());
}

