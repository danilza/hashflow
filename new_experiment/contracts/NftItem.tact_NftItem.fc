;; NftItem.tact_NftItem.storage.fc
;;
;; Type: NftItem
;; TLB: _ owner:address index:int257 pipeline_hash:int257 pipeline_raw:^slice collection:address = NftItem
;;

builder $NftItem$_store(builder build_0, (slice, int, int, slice, slice) v) inline {
    var (v'owner, v'index, v'pipeline_hash, v'pipeline_raw, v'collection) = v;
    build_0 = build_0.store_slice(v'owner);
    build_0 = build_0.store_int(v'index, 257);
    build_0 = build_0.store_int(v'pipeline_hash, 257);
    build_0 = build_0.store_builder_ref(begin_cell().store_slice(v'pipeline_raw));
    var build_1 = begin_cell();
    build_1 = build_1.store_slice(v'collection);
    build_0 = store_builder_ref(build_0, build_1);
    return build_0;
}

(slice, ((slice, int, int, slice, slice))) $NftItem$_load(slice sc_0) inline {
    var v'owner = sc_0~load_msg_addr();
    var v'index = sc_0~load_int(257);
    var v'pipeline_hash = sc_0~load_int(257);
    var v'pipeline_raw = sc_0~load_ref().begin_parse();
    slice sc_1 = sc_0~load_ref().begin_parse();
    var v'collection = sc_1~load_msg_addr();
    return (sc_0, (v'owner, v'index, v'pipeline_hash, v'pipeline_raw, v'collection));
}

(slice, ((int, slice, int, slice, slice))) $NftItem$init$_load(slice sc_0) inline {
